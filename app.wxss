/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.pull-left{
	float: left;
}
.pull-right{
	float: right;
}
.toastHidden{
  color: #EEE;
  text-align: center;
}
.gap{
  height: 35rpx;
  width: 100%;
  text-align: center;
  padding:10rpx 0;
  font-size: 28rpx;
  background-color: #5bc0de;
  border-color:#46b8da;
  color: #fff;
  position: relative;
}
#loadingLogo{
  width:100%;
  height: 100%;
  position: fixed;
  top:0;
  left:0;
  background-color: white;
}
#loadingLogo .title{
  font-style: oblique;
  font-size: 50rpx;
  color: #E6E8FA95;
  margin-top: 50%;
  text-align: center;
}
.bg{
  background:#aef;
}

.bg-2{
  background:#eaf;
}

.bg-3{
  background:#aeb;
}

.bg-4{
  background:#9a9;
}

.bg-5{
